<?xml version="1.0" encoding="UTF-8"?>
<soapenv:Body xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <NotifyCaseEvent xmlns="urn:oasis:names:tc:legalxml-courtfiling:wsdl:WebServiceMessagingProfile-Definitions-4.0">
    <NotifyCaseEventRequest>
      <NotifyCaseEventMessage xmlns="urn:tyler:ecf:extensions:CaseNotifyMessage">

        <!-- Use DocumentSecurity when a document’s visibility or access changes (e.g., Public → Private). -->
        <!-- re:Search Action: Runs GetCase to refresh document security. -->
        <!-- UI Result: Document disappears, unaccessible or appears based on permissions. -->

        <CaseCourt xmlns:nc="http://niem.gov/niem/niem-core/2.0">
          <nc:OrganizationIdentification>
            <nc:IdentificationID>generic-court</nc:IdentificationID>
          </nc:OrganizationIdentification>
        </CaseCourt>

        <CaseTrackingID xmlns="http://niem.gov/niem/niem-core/2.0">CASE-000009</CaseTrackingID>
        <NotificationReason>Integration Test</NotificationReason>

        <Event>
          <ID>EVT-009</ID>
          <EventType>DocumentSecurity</EventType>
        </Event>

      </NotifyCaseEventMessage>
    </NotifyCaseEventRequest>
  </NotifyCaseEvent>
</soapenv:Body>
